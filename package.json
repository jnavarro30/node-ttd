{
  "name": "ttd",
  "version": "1.0.0",
  "description": "test driven development",
  "main": "app.js",
  "scripts": {
    "start": "nodemon index",
    "test": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnavarro30/node-ttd.git"
  },
  "author": "Jesse Navarro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jnavarro30/node-ttd/issues"
  },
  "homepage": "https://github.com/jnavarro30/node-ttd#readme",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.33.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "semi": "warn",
      "eqeqeq": "warn",
      "backtick": "off",
      "quotes": [
        "warn",
        "single"
      ],
      "parser": "flow"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
